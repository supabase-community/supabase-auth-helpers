		"build": "tsup",
		"clean:all": "rimraf dist node_modules"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/supabase/auth-helpers.git"
	},
	"keywords": [
		"Supabase",
		"Auth",
		"Svelte Kit",
		"Svelte"
