{
  "name": "@supabase/auth-helpers-sveltekit",
  "version": "0.6.11",
  "description": "A collection of framework specific Auth utilities for working with Supabase.",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "linkDirectory": true
  },
  "scripts": {
    "build": "svelte-kit sync && svelte-package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supabase/auth-helpers.git"
  },
  "keywords": [
    "Supabase",
    "Auth",
    "Svelte Kit",
    "Svelte"
  ],
  "author": "Supabase",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/supabase/auth-helpers/issues"
  },
  "homepage": "https://github.com/supabase/auth-helpers/tree/main/packages/sveltekit#readme",
  "devDependencies": {
    "@sveltejs/kit": "1.0.0-next.499",
    "@sveltejs/package": "1.0.0-next.5",
    "svelte": "^3.48.0",
    "svelte2tsx": "^0.5.16",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^3.1.0",
    "@supabase/supabase-js": "^1.35.3"
  },
  "dependencies": {
    "@supabase/auth-helpers-shared": "workspace:*"
  },
  "peerDependencies": {
    "@supabase/supabase-js": "^1.35.3"
  }
}
